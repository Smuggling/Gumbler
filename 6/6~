		jQuery(function(){
			st= parseInt(((new Date()).getTime()-st)/10);
			var ut=(new Date()).getTime();
			jQuery.get('/img/p.gif?r'+(new Date()).getTime(), function(){
				ut=parseInt(((new Date()).getTime()-ut)/10);
				jQuery.get('/img/s.gif?ut='+ut+'&st='+st);
			});
		});
