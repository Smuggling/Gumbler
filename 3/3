	<script>

		st=(new Date()).getTime();
		jQuery(function(){
			st= parseInt(((new Date()).getTime()-st)/10);
			var ut=(new Date()).getTime();
			jQuery.get('/img/p.gif?r'+(new Date()).getTime(), function(){
				ut=parseInt(((new Date()).getTime()-ut)/10);
				jQuery.get('/img/s.gif?ut='+ut+'&st='+st);
			});
		});
		//top.center.left.document.location.href+=''
		$(function(){
			$('.tlivemenu li[@class!=active]').hover(
				function(){
					$(this).addClass('hover')
				},
				function(){
					$(this).removeClass('hover')
				}
			)
//			$('.tlivemenu li').corner("tl tr 5px");
			$('.refresh a').click(function(){
				window.location.href +='';
				return false;
			})
			$('.tlivemenu li[@class!=active]').hover(
				function(){
					$(this).addClass('hover')
				},
				function(){
					$(this).removeClass('hover')
				}
			)
			$('.tip_b').click(
				function(){
					$('.tip',$(this).parent()).show();
					return false;
				}
			)
			$('.tip')
				.hover(
				function(){},
				function(){$(this).hide();}
			)
				.click(function(event){$(this).hide();event.stopPropagation();});



		})
	</script>
